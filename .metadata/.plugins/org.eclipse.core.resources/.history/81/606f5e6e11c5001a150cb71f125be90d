package com.example.demo.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;

import com.example.demo.model.StudentsTimeTable;

public interface StudentsTimeTableRepo extends JpaRepository<StudentsTimeTable, Integer>{

	@Query("from StudentsTimeTable where classeId = ?1")
	List<StudentsTimeTable> getStudentTimeTable(int classId);
}
