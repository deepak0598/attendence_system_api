package com.example.demo.modelforservices;

import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;


@Setter
@Getter
@AllArgsConstructor
@NoArgsConstructor
@ToString
public class StudentsTimeTableServiceModel {
	private String forClasse;
	private List<String> days=new ArrayList<String>();
	private List<String> lectures=new ArrayList<String>();
	private Set<String> monSubjects=new HashSet<String>();
	private Set<String> monTeachers=new HashSet<String>();
	private Set<String> tueSubjects=new HashSet<String>();
	private Set<String> tueTeachers=new HashSet<String>();
	private Set<String> wedTeachers=new HashSet<String>();
	private Set<String> wedSubjects=new HashSet<String>();
	private Set<String> thuTeachers=new HashSet<String>();
	private Set<String> thuSubjects=new HashSet<String>();
	private Set<String> friTeachers=new HashSet<String>();
	private Set<String> friSubjects=new HashSet<String>();
}
